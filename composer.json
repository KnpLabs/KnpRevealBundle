    "description": "Simple bundle that uses Reveal.js and Symfony to make maintainable presentations",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php":                      ">=5.4",
        "phpspec/phpspec": "2.0.*@dev"
    },
    "require-dev": {
        "behat/behat": "~3@dev",
        "behat/mink-extension": "~2@dev",
        "behat/mink-selenium2-driver": "*",
        "symfony/symfony": "~2.4"
    },
    "autoload": {
        "psr-0": { "Knp\\RevealBundle": "" }
    },
    "target-dir": "Knp/RevealBundle"
}
